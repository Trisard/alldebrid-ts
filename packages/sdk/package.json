{
  "name": "@adbjs/sdk",
  "type": "module",
  "version": "2.1.1",
  "description": "Modern TypeScript SDK for AllDebrid API",
  "author": "Trisard",
  "license": "MIT",
  "keywords": [
    "alldebrid",
    "debrid",
    "api",
    "sdk",
    "typescript",
    "download",
    "premium",
    "torrent",
    "magnet"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trisard/alldebrid-ts.git"
  },
  "homepage": "https://github.com/Trisard/alldebrid-ts#readme",
  "bugs": {
    "url": "https://github.com/Trisard/alldebrid-ts/issues"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "bun run build:esm && bun run build:cjs && bun run build:types",
    "build:esm": "bun build ./src/index.ts --outdir ./dist --target node",
    "build:cjs": "bun build ./src/index.ts --outfile ./dist/index.cjs --target node",
    "build:types": "dts-bundle-generator -o dist/index.d.ts src/index.ts && cp dist/index.d.ts dist/index.d.cts",
    "dev": "bun run build --watch",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "generate": "bun run scripts/generate-types.js"
  },
  "dependencies": {
    "wretch": "^3.0.3"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.55.0",
    "@types/node": "^24.10.1",
    "dts-bundle-generator": "^9.5.1",
    "typescript": "^5.7.2",
    "valibot": "^0.42.1"
  }
}
